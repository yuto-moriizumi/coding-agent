{
  "name": "coding-agent",
  "displayName": "coding-agent",
  "description": "",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.100.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "publisher": "volgakurvar",
  "repository": "https://github.com/yuto-moriizumi/coding-agent",
  "contributes": {
    "commands": [
      {
        "command": "code-tutor.annotate",
        "title": "Toggle Tutor Annotations"
      },
      {
        "command": "codingAgent.openChat",
        "title": "Open CodingAgent Chat",
        "icon": "$(comment-discussion)"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "codingAgent",
          "title": "CodingAgent",
          "icon": "$(robot)"
        }
      ]
    },
    "views": {
      "codingAgent": [
        {
          "id": "codingAgentChat",
          "name": "Chat",
          "type": "webview",
          "retainContextWhenHidden": true
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "codingAgent.openChat",
          "when": "view == codingAgentChat",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md,css}\""
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "24.x",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.6",
    "@types/vscode": "^1.100.0",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2",
    "esbuild": "^0.25.3",
    "eslint": "^9.25.1",
    "eslint-config-prettier": "^10.1.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.5.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@langchain/core": "^0.3.57",
    "@langchain/langgraph": "^1.0.0",
    "@langchain/openai": "^0.6.1",
    "dotenv": "^17.1.0",
    "langchain": "^0.3.27",
    "zod": "^4.0.5"
  }
}
